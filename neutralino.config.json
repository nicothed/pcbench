{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/dev/schemas/neutralino.config.schema.json",
  "applicationId": "com.pcbench.pcbench",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "computer.*",
    "filesystem.*",
    "storage.*",
    "debug.*",
    "window.*"
  ],
  "modes": {
    "window": {
      "title": "PC Benchmark Tool",
      "width": 950,
      "height": 800,
      "minWidth": 800,
      "minHeight": 600,
      "fullScreen": false,
      "alwaysOnTop": false,
      "iconOLD": "/resources/icon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "PCBench",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0"
  }
}